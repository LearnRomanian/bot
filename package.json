    "linguistics",
    "dictionaries",
    "translators",
    "detectors",
    "localised"
  ],
  "homepage": "https://logos.wordcollector.co.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/vxern/logos"
  },
  "bugs": {
    "url": "https://github.com/vxern/logos",
    "email": "vxern@wordcollector.co.uk"
  },
  "contributors": [
    {
      "name": "Dorian \"vxern\" OszczÄ™da",
      "email": "vxern@wordcollector.co.uk",
      "url": "https://github.com/vxern"
    }
  ],
  "scripts": {
    "get": "npm install && npm audit fix",
    "provision": "tsx src/scripts/provision-sentence-pairs.ts",
    "setup": "npm run get && npm run provision",
    "#": "",
    "dev": "pm2 start ecosystem.config.cjs --instances 1 --no-autorestart --no-daemon",
    "format": "biome check src/ --apply-unsafe --organize-imports-enabled true",
    "##": "",
    "start": "pm2 start ecosystem.config.cjs --instances 1 --exp-backoff-restart-delay=500"
  },
  "dependencies": {
    "@discordeno/bot": "^19.0.0-next",
