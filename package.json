    "fancy-log": "^2.0.0",
    "ioredis": "^5.3.2",
    "lavaclient": "^4.1.1",
    "object-hash": "^3.0.0",
    "parse-wiktionary": "^1.0.0",
    "ravendb": "^5.4.1",
    "tinyld": "^1.3.4",
    "youtube-sr": "^4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
