{
  "$schema": "https://json.schemastore.org/package",
  "name": "logos",
  "description": "A multi-purpose community bot built to cater to language-learning communities on Discord.",
  "license": "ISC",
  "version": "3.38.0",
  "type": "module",
  "keywords": [
    "discord",
    "bot",
    "definitions",
