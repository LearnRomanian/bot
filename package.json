    "fasttext.wasm.js": "^1.0.0",
    "ioredis": "^5.4.1",
    "mongodb": "^6.8.0",
    "nano": "^10.1.4",
    "nanoid": "^5.0.7",
    "object-hash": "^3.0.0",
